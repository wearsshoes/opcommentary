{
  "name": "Octopus Pie Commentary",
  "version": "1.0",
  "description": "Switches between Octopus Pie with original run comments and the rerun with author commentary",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/opcommentary16.png",
      "32": "images/opcommentary32.png",
      "48": "images/opcommentary48.png",
      "128": "images/opcommentary128.png"
    }
  },
  "icons": {
    "16": "images/opcommentary16.png",
    "32": "images/opcommentary32.png",
    "48": "images/opcommentary48.png",
    "128": "images/opcommentary128.png"
  },
  "manifest_version": 2
}
